echo "* text=auto" > .gitattributes
echo "*.php text eol=lf" >> .gitattributes
echo "*.blade.php text eol=lf" >> .gitattributes
echo "*.css text eol=lf" >> .gitattributes
echo "*.js text eol=lf" >> .gitattributes
echo "*.sql text eol=lf" >> .gitattributes
git add .gitattributes
git commit -m "Menambahkan .gitattributes untuk mengatur akhir baris"
git push origin main
